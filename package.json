{
  "name": "@foxglove/rosbag",
  "version": "3.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/foxglove/rosbag.git"
  },
  "dependencies": {
    "buffer": "6.0.3",
    "heap": "0.2.6",
    "int53": "1.0.0",
    "web-encoding": "1.1.5"
  },
  "main": "dist/node/index.js",
  "browser": "dist/web/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "lint": "eslint . --ext .ts",
