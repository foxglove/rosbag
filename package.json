  "devDependencies": {
    "@types/heap": "0.2.28",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "compressjs": "1.0.3",
    "cross-env": "5.2.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "27.0.6",
    "jest-cli": "27.0.6",
    "lz4js": "0.2.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "typedoc": "0.20.23",
    "typescript": "4.1.3"
  },
  "prettier": {
    "arrowParens": "always",
    "jsxBracketSameLine": true,
